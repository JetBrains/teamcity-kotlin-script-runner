<?xml version="1.0" encoding="UTF-8"?>
<beans
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:util="http://www.springframework.org/schema/util"
    xmlns:lang="http://www.springframework.org/schema/lang"
    xsi:schemaLocation="
  http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
  http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.0.xsd
  http://www.springframework.org/schema/lang http://www.springframework.org/schema/lang/spring-lang-2.0.xsd
  "
    default-autowire="constructor"
    >
  <bean id="kotlinStepRunnerInfo" class="jetbrains.buildServer.runner.kotlinBuildStep.KotlinStepBuildRunnerInfo"/>
  <bean id="kotlinStepRunnerFactory" class="jetbrains.buildServer.runner.kotlinBuildStep.KotlinStepRunnerFactory"/>
  <bean id="kotlinToolProvider" class="jetbrains.buildServer.runner.kotlinBuildStep.KotlinToolProvider"/>
</beans>
